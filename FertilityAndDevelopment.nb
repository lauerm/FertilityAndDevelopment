Cell[TextData[{
 "Define a function to generate a path for both development and fertility in \
the given country, beginning at the values in the country's reference year, \
but remove the part of the path prior to reaching the HDI threshold according \
to the paper's definition.  \nReturn the resulting path as a list of pairs, \
expressed relative to the starting point.\nLet the HDI evolve with drift and \
vol equal to the overall values for advanced countries found above.   \nLet \
the TFR evolve with vol equal to the overall value for advanced countries \
found above,\n",
 StyleBox["but with zero drift.\n",
  FontSlant->"Italic"],
 "Warning: Uses global variables HDIDrift, HDIVol, TFRVol"
}], "Text",
 CellChangeTimes->{{3.461235560865762*^9, 3.461235826214526*^9}, {
  3.461236671172301*^9, 3.461236694385312*^9}, {3.461251314726227*^9, 
  3.461251329477395*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"GeneratePostThresholdPath", "[", 
   RowBox[{
   "country_String", ",", " ", "windowstart_Real", ",", " ", 
    "windowend_Real"}], "]"}], " ", ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", " ", 
     RowBox[{"y0", ",", " ", "f", ",", " ", "d", ",", " ", "threshold"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"Find", " ", "reference", " ", "year"}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"y0", " ", "=", " ", 
      RowBox[{"ReferenceYear", "[", 
       RowBox[{"country", ",", " ", "windowstart"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Generate", " ", "complete", " ", "path", " ", "from", " ", "reference",
        " ", "year"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"d", " ", "=", " ", 
      RowBox[{
       RowBox[{"GeneratePath", "[", 
        RowBox[{
         RowBox[{"NormalDistribution", "[", 
          RowBox[{"HDIDrift", ",", " ", "HDIVol"}], "]"}], ",", " ", 
         RowBox[{"2005", "-", "y0"}]}], "]"}], "\[IndentingNewLine]", "+", 
       RowBox[{"HDI", "[", 
        RowBox[{"country", ",", " ", "y0"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"f", " ", "=", " ", 
      RowBox[{
       RowBox[{"GeneratePath", "[", 
        RowBox[{
         RowBox[{"NormalDistribution", "[", 
          RowBox[{"0", ",", " ", "TFRVol"}], "]"}], ",", " ", 
         RowBox[{"2005", "-", "y0"}]}], "]"}], "\[IndentingNewLine]", "+", 
       RowBox[{"TFR", "[", 
        RowBox[{"country", ",", " ", "y0"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Determine", " ", "when", " ", "country", " ", "passes", " ", 
       "threshold", " ", "according", " ", "to", " ", "paper"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"threshold", " ", "=", " ", 
      RowBox[{"FindThreshold", "[", 
       RowBox[{
       "d", ",", " ", "f", ",", " ", "windowstart", ",", " ", "windowend"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Remove", " ", "ealier", " ", "part", " ", "of", " ", "path"}], 
      " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"d", " ", "=", " ", 
      RowBox[{"Drop", "[", 
       RowBox[{"d", ",", " ", 
        RowBox[{"threshold", " ", "-", "1"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"f", " ", "=", " ", 
      RowBox[{"Drop", "[", 
       RowBox[{"f", ",", " ", 
        RowBox[{"threshold", " ", "-", "1"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Transpose", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"d", "-", 
         RowBox[{"First", "[", "d", "]"}]}], ",", " ", 
        RowBox[{"f", "-", 
         RowBox[{"First", "[", "f", "]"}]}]}], "}"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
