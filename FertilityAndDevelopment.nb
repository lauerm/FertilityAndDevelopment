Cell["\<\
Define a function that takes the development and fertility paths of a \
country, and implements the threshold finding rule given in the paper with \
respect to a given window, namely find the lowest value of TFR while the \
country's HDI is within the window, and set the threshold equal to the HDI \
for the first year in the window for which this lowest value is observed.\
\>", "Text",
 CellChangeTimes->{{3.46123471838987*^9, 3.461234853535225*^9}, {
  3.461234896009722*^9, 3.461234910514787*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"FindThreshold", "[", 
   RowBox[{
   "development_List", ",", " ", "fertility_List", ",", " ", 
    "windowstart_Real", ",", " ", "windowend_Real"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"eligible", ",", " ", "lowestvalue", ",", " ", "candidates"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Find", " ", "positions", " ", "where", " ", "development", " ", "is", 
      " ", "within", " ", "window"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"eligible", "=", " ", 
      RowBox[{"Position", "[", 
       RowBox[{"development", ",", " ", 
        RowBox[{"d_Real", " ", "/;", " ", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"d", " ", "\[GreaterEqual]", " ", "windowstart"}], "&&", 
           " ", 
           RowBox[{"d", " ", "\[LessEqual]", " ", "windowend"}]}], ")"}]}], 
        ",", " ", 
        RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Return", " ", "last", " ", "position", " ", "if", " ", "there", " ", 
       "are", " ", "no", " ", "eligible", " ", "ones"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"eligible", " ", "\[Equal]", " ", 
        RowBox[{"{", "}"}]}], ",", " ", 
       RowBox[{"Return", "[", 
        RowBox[{"Length", "[", "fertility", "]"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Determine", " ", "lowest", " ", "fertility", " ", "amongst", " ", 
       "eligible", " ", "positions"}], " ", "*)"}], " ", 
     "\[IndentingNewLine]", 
     RowBox[{"lowestvalue", " ", "=", " ", 
      RowBox[{"Min", "[", 
       RowBox[{"Extract", "[", 
        RowBox[{"fertility", ",", " ", "eligible"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Find", " ", "positions", " ", "of", " ", "this", " ", "lowest", " ", 
       "fertility"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"candidates", " ", "=", " ", 
      RowBox[{"Position", "[", 
       RowBox[{"fertility", ",", " ", "lowestvalue", ",", " ", 
        RowBox[{"{", "1", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Return", " ", "first", " ", "such", " ", "position", " ", "that", " ", 
       "is", " ", "eligible"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"First", "[", 
      RowBox[{"First", "[", 
       RowBox[{"Intersection", "[", 
        RowBox[{"candidates", ",", " ", "eligible"}], "]"}], " ", "]"}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
