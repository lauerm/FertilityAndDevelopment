Cell[BoxData[
 RowBox[{
  RowBox[{"Store", "[", 
   RowBox[{"value_", ",", " ", 
    RowBox[{"{", 
     RowBox[{"country_Integer", ",", " ", "columnname_Integer"}], "}"}]}], 
   "]"}], " ", ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"type", ",", " ", "year"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"type", ",", " ", "year"}], "}"}], " ", "=", " ", 
      RowBox[{"StringSplit", "[", 
       RowBox[{
        RowBox[{"headinglist", "[", 
         RowBox[{"[", "columnname", "]"}], "]"}], ",", " ", "\"\<.\>\""}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Symbol", "[", "type", "]"}], "[", 
        RowBox[{
         RowBox[{"countrylist", "[", 
          RowBox[{"[", "country", "]"}], "]"}], ",", " ", 
         RowBox[{"ToExpression", "[", "year", "]"}]}], "]"}], ")"}], " ", "=",
       " ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"NumberQ", "[", "value", "]"}], ",", " ", "value", ",", " ", 
        RowBox[{"Missing", "[", "]"}]}], "]"}]}]}]}], "\[IndentingNewLine]", 
