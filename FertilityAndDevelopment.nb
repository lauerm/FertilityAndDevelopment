Cell["\<\
Define a function to generate paths according to a given step distribution.
Note that paths will have (steps+1) elements, the first always being zero;
in this way, the caller can add a constant to change the start value\
\>", "Text",
 CellChangeTimes->{{3.461209125639802*^9, 3.461209144082774*^9}, {
  3.461209706913067*^9, 3.461209727830305*^9}, {3.461235316708298*^9, 
  3.461235400306775*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"GeneratePath", "[", 
   RowBox[{"distribution_", ",", " ", "steps_Integer"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "changes", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"changes", " ", "=", " ", 
      RowBox[{"RandomReal", "[", 
       RowBox[{"distribution", ",", " ", "steps"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"FoldList", "[", 
      RowBox[{"Plus", ",", " ", "0", ",", " ", "changes"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.46120866450487*^9, 3.461208765273569*^9}}],

